{
  "name": "js-validate",
  "version": "1.0.0",
  "description": "Javascript object validation &amp; Express middleware.",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/unit/*",
    "test-integ": "mocha test/integ/*",
    "docs:generate": "documentation build --lint -f html -o docs index.js",
    "docs:publish": "npm run docs:generate && git add docs && git commit --allow-empty -m 'chore(docs): updated docs'",
    "docs:serve": "nodemon --watch docs --exec \"http-server\" docs",
    "release": "standard-version",
    "publish": "npm run release && reggie -u http://pkg.prod.airmap.com:7777 publish "
  },
  "license": "UNLICENSED",
  "dependencies": {
    "bunyan": "^1.8.1",
    "config-node": "^1.2.2",
    "dotenv": "^2.0.0",
    "lodash": "^4.16.3",
    "moment": "^2.15.1",
    "sinon": "^1.17.6",
    "validate.js": "^0.10.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "documentation": "^4.0.0-beta10",
    "http-server": "^0.9.0",
    "mocha": "^2.5.3",
    "mockery": "^1.7.0",
    "nodemon": "^1.10.2",
    "sinon": "^1.17.4",
    "standard-version": "^2.3.1"
  }
}
